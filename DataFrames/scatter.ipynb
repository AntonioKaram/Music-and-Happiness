    "import pandas as pd\n",
    "import plotly.express as px\n",
    "\n",
    "store = pd.HDFStore('store.h5')\n",
    "\n",
    "data = pd.read_pickle('store.pkl')\n",
    "\n",
    "figure_animated_all = px.scatter(data, x='avg valence', y='depression', size = 'gdp per cap' ,animation_frame = 'year')\n",
    "figure_animated_all.show()\n",
    "\n"
   ]
  }
